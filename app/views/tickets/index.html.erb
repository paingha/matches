
 
<div class="app layout-fixed-header">
<%= render 'sidebar' =%>
 
 
<div class="main-panel">
 
<%= render 'header' =%>
 
<div class="main-content">
<div class="m-x-n-g m-t-n-g overflow-hidden">
<div class="row">
<div class="col-sm-12">
<div class="card card-block bg-white">
<h4 class="card-title text-center">TICKETS<div class="pull-right">
<a href="/tickets/new" class="btn btn-primary">New Ticket</a>
</div></h4>

</div>
</div>
</div>


  </div>
<div class="row same-height-cards">

<% @tickets.each do |ticket| %>
<div class="col-md-4">
<div class="card no-border bg-white">
<div class="card-block">
<h4 class="card-title"><strong><%= link_to ticket.title, ticket_path(ticket) %></strong></h4>
<div class="p-a">
<p><%= sanitize(ticket.message.truncate(180, separator: '</p>')) %></p>
</div>
<div class="">
<a href="#" class="card-link pull-right"><i class="icon-clock"></i>&nbsp;<strong><%= time_ago_in_words(ticket.created_at) %>&nbsp;ago</strong></a>
<a href="#" class="card-link m-l-0"><% if ticket.priority == "Low" %><strong><font color = "green">Priority : Low</font></strong> <% elsif ticket.priority == "Medium" %><strong><font color = "orange">Priority : Medium</font></strong> <% elsif ticket.priority == "High" %><strong><font color = "red">Priority : High</font></strong><% end %></a>
</div>
</div>
</div>
</div>

<% end %>

</div>
</div>
 
</div>
 
 
<footer class="content-footer">
<nav class="footer-right">
<ul class="nav">
<li>
<a href="javascript:;">Feedback</a>
</li>
<li>
<a href="javascript:;" class="scroll-up">
<i class="fa fa-angle-up"></i>
</a>
</li>
</ul>
</nav>
<nav class="footer-left hidden-xs">
<ul class="nav">
<li>
<a href="javascript:;"><span>About</span> PayBox</a>
</li>
<li>
<a href="javascript:;">Privacy</a>
</li>
<li>
<a href="javascript:;">Terms</a>
</li>
<li>
<a href="javascript:;">Help</a>
</li>
</ul>
</nav>
</footer>
 

 
</div>

