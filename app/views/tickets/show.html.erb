
 
<div class="app layout-fixed-header">
<%= render 'sidebar' =%>
 
 
<div class="main-panel">

<%= render 'header' =%>

<div class="main-content">
<div class="m-x-n-g m-t-n-g overflow-hidden">
<div class="row">
<div class="col-sm-12">
<div class="card card-block bg-white">
<h4 class="card-title text-center">TICKETS<div class="pull-right"><a href="javascript:void(Tawk_API.toggle())"><button class="btn btn-primary">Support Chat</button></a></div></h4>

</div>
</div>
</div>


  </div>
<div class="row same-height-cards">
<div class="col-md-1"></div>
<div class="col-md-10">
<div class="card no-border bg-white">
<div class="card-block">
<div class="p-a">
<h4><%= @ticket.title %></h4>
<p><%= @ticket.message %></p>
</div>
<div class="pull-left">
  <% if @ticket.priority == "Low" %><strong><font color = "green">Priority : Low</font></strong> <% elsif @ticket.priority == "Medium" %><strong><font color = "orange">Priority : Medium</font></strong> <% elsif @ticket.priority == "High" %><strong><font color = "red">Priority : High</font></strong><% end %>
</div>
<div class="">

<a href="#" class="card-link pull-right"><i class="icon-clock"></i>&nbsp;<strong><%= time_ago_in_words(@ticket.created_at) %>&nbsp;ago</strong></a>

<br>
</div>
</div>
</div>
</div>
<div class="col-md-1"></div>



</div>
<% @ticket.comments.each do |comment| %>

<div class="row same-height-cards">
<div class="col-md-2"></div>
<div class="col-md-8">
<div class="card no-border bg-white">
<div class="card-block">
<div class="p-a">
<h4>Ticket Comment by <%= comment.user %></h4>
<p><%= comment.body %></p>
</div>
<div class="">


</div>
</div>
</div>
</div>
<div class="col-md-2"></div>



</div>


<% end %>
<%= render partial: "comments/form", locals: {commentable: @ticket} %>
</div>
 
</div>
 
 
<footer class="content-footer">
<nav class="footer-right">
<ul class="nav">
<li>
<a href="javascript:;">Feedback</a>
</li>
<li>
<a href="javascript:;" class="scroll-up">
<i class="fa fa-angle-up"></i>
</a>
</li>
</ul>
</nav>
<nav class="footer-left hidden-xs">
<ul class="nav">
<li>
<a href="javascript:;"><span>About</span> PayBox</a>
</li>
<li>
<a href="javascript:;">Privacy</a>
</li>
<li>
<a href="javascript:;">Terms</a>
</li>
<li>
<a href="javascript:;">Help</a>
</li>
</ul>
</nav>
</footer>
 

 
</div>

